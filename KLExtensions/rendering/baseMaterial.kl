namespace GorGon::Rendering
{
    object BaseMaterial
    {
        String name;
        GorGon::Core::Properties _properties;
    };

    protected BaseMaterial.init!(GorGon::Core::Properties p)
    {
        this._properties = p;
    }

    BaseMaterial()
    {
    }

    BaseMaterial(GorGon::Core::Properties p)
    {
        this.init(p);
    }
}

