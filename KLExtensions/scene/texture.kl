using namespace GorGon::Core;

namespace GorGon::Scene
{
    object CCTexture
    {
      String _path;
      Properties _properties;
    };

    CCTexture(String path, Properties p)
    {
      this._path = path;
      this._properties = p;
    }

    String CCTexture.getPath()
    {
      return this._path;
    }

    Properties CCTexture.getProperties()
    {
      return this._properties;
    }
}
